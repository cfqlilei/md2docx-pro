            "console": "integratedTerminal",
            "preLaunchTask": "build-complete-app-macos"
        },
        {
            "name": "启动完整应用 (Windows)",
            "type": "node",
            "request": "launch",
            "name": "Launch Complete App",
            "program": "${workspaceFolder}/scripts/launch_complete_app_windows.js",
            "console": "integratedTerminal",
            "preLaunchTask": "build-complete-app-windows"
        }
    ],
    "compounds": [
        {
            "name": "启动前后端 (macOS)",
            "configurations": [
                "启动Go后端服务 (macOS)",
