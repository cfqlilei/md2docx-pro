{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "启动Go后端服务 (macOS)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/server/main.go",
            "cwd": "${workspaceFolder}",
            "env": {
                "CGO_ENABLED": "1",
                "GOOS": "darwin",
                "GOARCH": "arm64"
            },
            "args": [],
            "showLog": true,
            "console": "integratedTerminal",
            "preLaunchTask": "go-mod-tidy"
        },
        {
            "name": "启动Go后端服务 (Windows)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/server/main.go",
            "cwd": "${workspaceFolder}",
            "env": {
                "CGO_ENABLED": "1",
                "GOOS": "windows",
                "GOARCH": "amd64"
            },
            "args": [],
            "showLog": true,
            "console": "integratedTerminal",
            "preLaunchTask": "go-mod-tidy"
        },
        {
            "name": "调试Go后端服务 (macOS)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/server/main.go",
            "cwd": "${workspaceFolder}",
            "env": {
                "CGO_ENABLED": "1",
                "GOOS": "darwin",
                "GOARCH": "arm64"
            },
            "args": [],
            "showLog": true,
            "console": "integratedTerminal",
            "preLaunchTask": "go-mod-tidy",
            "stopOnEntry": false,
            "dlvFlags": [
                "--check-go-version=false"
            ]
        },
        {
            "name": "调试Go后端服务 (Windows)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/server/main.go",
            "cwd": "${workspaceFolder}",
            "env": {
                "CGO_ENABLED": "1",
                "GOOS": "windows",
                "GOARCH": "amd64"
            },
            "args": [],
            "showLog": true,
            "console": "integratedTerminal",
            "preLaunchTask": "go-mod-tidy",
            "stopOnEntry": false,
            "dlvFlags": [
                "--check-go-version=false"
            ]
        },
        {
            "name": "启动Qt前端 (macOS)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/qt-frontend/build_md2docx_app/build/md2docx_app.app/Contents/MacOS/md2docx_app",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/qt-frontend/build_md2docx_app",
            "environment": [
                {
                    "name": "PATH",
                    "value": "/opt/homebrew/opt/qt@5/bin:${env:PATH}"
                }
            ],
            "externalConsole": false,
            "MIMode": "lldb",
            "preLaunchTask": "build-qt-complete-macos"
        },
        {
            "name": "启动Qt前端 (Windows)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/qt-frontend/build_md2docx_app/build/md2docx_app.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/qt-frontend/build_md2docx_app",
            "environment": [
                {
                    "name": "PATH",
                    "value": "C:\\Qt\\5.15.2\\msvc2019_64\\bin;${env:PATH}"
                }
            ],
            "console": "externalTerminal",
            "preLaunchTask": "build-qt-complete-windows"
        },
        {
